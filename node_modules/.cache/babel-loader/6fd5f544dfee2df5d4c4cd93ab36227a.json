,WAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KAXA;AAYA,IAAA,OAAA,EAAA,mBAAA;AAAA;;AAAA;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,KAAA,MAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,GAAA,CAAA,MAAA,CAAA,IAAA,EAFA,CAEA;;AACA,OAJA;AAKA,KAnBA;AAoBA,IAAA,YAAA,EAAA,wBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAtBA;AAuBA,IAAA,QAAA,EAAA,oBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AAzBA;AAlCA,CAAA","sourcesContent":["<template>\n  <div>\n\n      <!-- navbar  -->\n\n    <!-- content  -->\n\n    <!-- header  -->\n    <div class=\"header\"> \n            <div id=\"pic-header\" >\n                    <div class=\"main-header\">\n                        <div id=\"slogan\">\n                            <h1>My blog</h1>\n                            <p>blog cua Truong</p>\n                         </div>\n\n                         <div id=\"category\">\n                            <ul v-for=\"category in categories\" :key=\"category.id\" >\n                                    <li @click=\"CATEGORY_ID(category.id), categoryPage()\">{{category.name}}</li>\n                                   \n                            </ul>\n                         </div>\n \n                    </div>\n            </div>\n \n    </div>\n\n    <!-- container  -->\n    <div class=\"container\" v-bind:postId=\"filter.postId = Post.id\" >\n        <div>\n            <div v-for=\"post in posts\" :key=\"post.id\" id=\"post\">\n                <!-- lay ra id cua bai viet  -->\n                    <h3 @click=\"POST(post) ,postPage(), getTags()\">{{post.title}}</h3> \n                    <p>{{post.content}}</p>\n                     <p v-for=\"tag in tags[0].tags\" :key=\"tag.id\">{{tag.name}}</p>\n            </div>\n\n            <div>\n                    <button v-for=\"page in totalPage\" :key=\"page\" \n                    @click.prevent=\"currentPage = page \" \n                    >{{page}}</button>\n                    \n            </div>\n        </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nimport network from '../core/network'\nimport { mapState, mapMutations } from 'vuex'\nexport default {\n    name: 'HomePage',\n    data() {\n        return {\n            currentPage: 1,\n            posts: [],\n            tags: [3],\n            categories: [],\n            totalPage: 0,\n            filter: {\n                postId: [2],\n            }\n        }\n    },\n    computed: mapState(['Post' , 'CategoryId']),\n    mounted() {\n        this.getPosts() // truy van du lieu tu api\n\n    },\n    watch: {\n        currentPage: function() { //hien du lieu khi thay doi currentPage\n            this.getPosts();\n        }\n    },\n    created() {  \n                \n            network.get('category' , this.categories)\n                .then( response => {\n                this.categories = response.data;\n                })\n                .catch( error => {\n                    console.log(error);\n            });\n    },\n    methods: {\n        ...mapMutations(['POST','CATEGORY_ID','TAG']),\n        getPosts: function(){\n            network.get('post?page=' + this.currentPage)\n            .then( response => {\n                this.posts = response.data.data;\n                this.totalPage = response.data.last_page;\n            })\n            .catch( error => {\n                console.log(error);\n            })\n        },\n        getTags: function(){ // lay ra tag cua bai viet\n            \n            network.get('tag' , this.filter)\n            .then(response => {\n                this.tags = response.data;\n                this.TAG(this.tags) // lay ra tag bai viet xong luu vao state (localStorage)\n            })\n        },\n        categoryPage: function() {\n            this.$router.push({name: 'category_page'});\n        },\n        postPage: function() {\n            this.$router.push({name: 'post'});\n        }\n\n    }\n}\n</script>\n\n<style scoped>\n\n    \n\n/* header  */\n     .header #pic-header {\n        width: 100%;\n        height: 500px;\n        background: url('../assets/header.jpg') no-repeat 0px 50px;\n        background-size: 100% 500px;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n    } \n    .header .main-header {\n        text-align: center;\n        color: white;\n    }\n    .header .main-header #slogan h1{\n        font-size: 70px;\n        margin-bottom: 30px;\n    }\n    .header .main-header #slogan p {\n        font-size: 20px;\n    }\n    .header .main-header #category {\n        margin:100px 0px -200px 0px;\n        display: flex;\n        }\n    .header .main-header #category ul li {\n        margin: 0px 5px;\n    }\n    .header .main-header #category ul li  {\n        color: white;\n        font-size: 20px;\n        padding: 10px;\n    }\n\n    .header .main-header #category ul li:hover {\n        color: rgb(212, 200, 184);\n        cursor: pointer;\n    }\n\n    /* container  */\n    .container {\n        width: 60%;\n        margin: 50px auto;\n    }\n    .container > div {\n        text-align: center;\n\n    }\n    .container #post {\n        margin: 50px 0px;\n    }\n    .container > div button {\n        margin: 5px;\n        padding: 6px 20px;\n        font-size: 20px;\n        border: 1px solid black;\n        border-radius: 5px;\n    }\n    .container > div button:hover {\n        background: green ;\n        cursor: pointer;\n        color: white;\n\n    }\n\n    /* post */\n     #post h3:hover {\n         cursor: pointer;\n     }\n</style>"],"sourceRoot":"src/components"}]}