ttp://localhost:81/api/'\n\nexport default {\n    getHeaders() { // check dang nhap\n        let token = window.localStorage.getItem('token');\n        if(token == null){\n            return{}\n        }\n        return { Authorization: 'Bearer ' + token}\n    },\n    get(url , params ) {\n\n          return  axios.get(apiUrl + url , {params} ,{headers: this.getHeaders()})\n          \n\n        },\n\n    put(url, data  ) {\n\n        return axios.put(apiUrl + url , data, {headers: this.getHeaders()}\n            \n           \n        );\n    },\n\n    post(url ,data ){\n\n        return axios.post(apiUrl+ url , data ,{headers: this.getHeaders()}\n            \n        );\n    },\n\n    delete(url) {\n        return axios.delete(apiUrl+ url ,\n            \n         \n        );\n    }\n             \n}\n\n"]}]}