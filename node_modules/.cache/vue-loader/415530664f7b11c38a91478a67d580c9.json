dung tìm kiếm ...\" v-model=\"search.key_search\" @keypress.enter=\"findPost()\">\n                <input type=\"submit\" @click=\"INPUT_SEARCH()\" >\n            </div>\n     </transition>\n    <div class=\"header\"> \n            <div id=\"pic-header\" >\n                    <div class=\"main-header\">\n                        <div id=\"slogan\">\n                            <h1>Time Write</h1>\n                            <p>blog của Trường</p>\n                         </div>\n                         <div id=\"category\">\n                            <ul v-for=\"category in categories\" :key=\"category.id\" >\n                                    <li @click=\"CATEGORY_ID(category.id), categoryPage()\">{{category.name}}</li>\n\n                            </ul>\n                         </div>\n \n                    </div>\n            </div>\n \n    </div>\n\n    <!-- container  -->\n    <div class=\"container\" v-bind:postId=\"filter.postId = Post.id\" >\n        <div>\n            <div v-for=\"post in posts\" :key=\"post.id\" id=\"post\">\n                <!-- lay ra id cua bai viet  -->\n                    <h3 @click=\"POST(post) ,postPage()\" :title=\"post.title\">{{post.title}}</h3> \n                    <p>{{post.short_content}}</p>\n            </div>\n\n            <div id=\"page\" >\n                <div v-for=\"page in totalPage\" :key=\"page\">\n                            <button  :class=\"{ isPage: currentPage==page }\" @click.prevent=\"currentPage = page \" >{{page}}</button>\n                </div>\n            </div>\n        </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport network from '../core/network'\nimport { mapState, mapMutations } from 'vuex'\nimport _ from 'lodash';\n\nexport default {\n    name: 'HomePage',\n    data() {\n        return {\n            currentPage: 1,\n            posts: [],\n            categories: [1],\n            totalPage: 0,\n            filter: {\n                postId: [2],\n            },\n            search: {\n                key_search: ''\n             }\n        }\n    },\n    computed: mapState(['Post' , 'CategoryId','input_search']),\n    mounted() {\n        this.getPosts(); // truy van du lieu tu api,\n\n    },\n    watch: {\n        currentPage: function() { //hien du lieu khi thay doi currentPage\n            this.getPosts();\n        },\n    },\n    created() {  \n  \n            network.get('category' , this.categories)\n                .then( response => {\n                this.categories = response.data;\n                this.CATEGORY(response.data);\n                })\n                .catch( error => {\n                    console.log(error);\n            });\n    },\n    methods: {\n        ...mapMutations(['POST','CATEGORY_ID','CATEGORY','INPUT_SEARCH']),\n        getPosts: _.debounce(function(){\n            network.get('post?page=' + this.currentPage)\n            .then( response => {\n                this.posts = response.data.data;\n                this.totalPage = response.data.last_page;\n            })\n            .catch( error => {\n                console.log(error);\n            })\n        },1000),\n        categoryPage: function() {\n            this.$router.push({name: 'category_page'});\n        },\n        postPage: function() {\n            this.$router.push({name: 'post'});\n        },\n        findPost: function(){ // tim kiem bai viet\n            network.get('findPost' , this.search)\n            .then(response => {\n                    this.posts = response.data\n                    console.log(response.data)\n            })\n        },\n\n    }\n}\n</script>\n\n<style scoped>\n\n    \n    #search {\n        position: fixed;\n        background: white;\n        top: 6%;\n        right: 15%;\n        padding: 10px;\n        z-index: 1000;\n    }\n    #search input[type=text] {\n        width: 500px;\n        height: 20px;\n        border: none;\n        border-bottom: 1px solid rgb(223, 207, 207);\n        padding: 0px 10px;\n        outline: none;\n        position: relative;\n    }\n    #search input[type=submit] {\n        content: ' ........';\n        position: absolute;\n        right: 10px;\n        width: 30px;\n        height: 30px;\n        background:url('../assets/close_input.png') no-repeat;\n        background-size: 12px 12px;\n        border: none;\n        outline: none;\n        text-indent: -10000px;\n    }\n/* header  */\n     .header #pic-header {\n        width: 100%;\n        height: 500px;\n        background: url('../assets/header.jpg') no-repeat 0px 50px;\n        background-size: 100% 500px;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n    } \n    .header .main-header {\n        text-align: center;\n    }\n    .header .main-header #slogan h1{\n        font-size: 70px;\n        margin-bottom: 20px;\n    }\n    .header .main-header #slogan p {\n        font-size: 20px;\n        margin-bottom: 100px;\n    }\n    .header .main-header #category {\n        margin:60px 0px -200px 0px;\n        display: flex;\n        }\n    .header .main-header #category ul li  {\n        font-size: 20px;\n        padding: 10px ;\n        font-weight: 600;\n    }\n\n    .header .main-header #category ul li:hover {\n        cursor: pointer;\n        border-radius: 4px;\n        background: rgb(233, 230, 224);\n    }\n\n    /* container  */\n    .container {\n        width: 60%;\n        margin: 0px auto;\n        text-align: left;\n    }\n    .container #post {\n        margin: 50px 0px;\n    }\n    .isPage{\n      background: black;\n      color: white;\n    }\n    #page  { \n          width: 70%;\n          margin: 20px auto;\n          display: flex;\n          justify-content: center\n    }\n    #page button {\n      width: 60px;\n      height: 40px;\n      margin: 0px 5px;\n    }\n    #page button:hover {\n        cursor: pointer;\n    }\n\n    /* post */\n    #post {\n        border-bottom: 1px solid rgb(219, 205, 205);\n        padding: 50px;\n    }\n     #post h3:hover {\n         cursor: pointer;\n     }\n</style>"]}]}